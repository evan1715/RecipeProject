  "scripts": {
    "start": "node server/server.js",
    "heroku-postbuild": "npm run build:prod",
    "db": "startDB.bat",
    "build:dev": "webpack --node-env development",
    "build:prod": "webpack --node-env production",
    "client": "webpack serve --node-env development",
    "server": "env-cmd -f server/config/dev.env nodemon server/server.js -e js,html,css,scss",
    "dev": "concurrently \"webpack --watch --node-env development\" \"env-cmd -f server/config/dev.env nodemon server/dev-server.js -e js,html,css,scss\"",
    "prod": "concurrently \"webpack --watch --node-env production\" \"env-cmd -f server/config/dev.env nodemon server/server.js -e js,html,css,scss\""
  },
